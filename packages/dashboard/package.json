{
  "name": "@ohbug-server/dashboard",
  "version": "0.0.0",
  "description": "ohbug dashboard server",
  "author": "chenyueban",
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "cross-env NODE_ENV=development nest start --watch --preserveWatchOutput\t",
    "build": "nest build",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/serve-static": "^2.1.4",
    "@ohbug-server/common": "^0.0.0",
    "cookie-parser": "^1.4.5",
    "nest-winston": "^1.3.4",
    "nestjs-redis": "^1.2.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "source-map-trace": "^0.1.3",
    "uuid": "^8.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.1.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/passport-jwt": "^3.0.4"
  }
}
